{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --configuration=ci",
    "test:firestore-rules": "firebase emulators:exec --only firestore \"jest --forceExit firestore/rules.spec.ts\"",
    "test:storage-rules": "firebase emulators:exec --only storage \"jest --forceExit cloud_storage/rules.spec.ts\"",
    "e2e": "firebase emulators:exec --only=auth \"ng e2e\" --import=\"./e2e/emulator-data/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.3",
    "@angular/common": "^18.0.3",
    "@angular/compiler": "^18.0.3",
    "@angular/core": "^18.0.3",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "^18.0.3",
    "@angular/platform-browser": "^18.0.3",
    "@angular/platform-browser-dynamic": "^18.0.3",
    "@angular/router": "^18.0.3",
    "rxfire": "6.0.5",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.4",
    "@angular/cli": "~18.0.4",
    "@angular/compiler-cli": "^18.0.3",
    "@babel/preset-typescript": "^7.24.7",
    "@firebase/rules-unit-testing": "^3.0.3",
    "@puppeteer/ng-schematics": "^0.6.0",
    "@types/jasmine": "~5.1.4",
    "jasmine": "5.1.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "puppeteer": "22.11.2",
    "typescript": ">=5.2.0 and <5.5.0"
  }
}
